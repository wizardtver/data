        "                'Воронеж',\n",
        "                'Пермь',\n",
        "                'Волгоград') \n",
        "                AND e.name = 'Яндекс'\n",
        "            GROUP BY a.name\n",
        "            UNION ALL\n",
        "            SELECT\n",
        "                'Total',\n",
