        "query_4_4 = f'''\n",
        "            SELECT\n",
        "                schedule,\n",
        "                employment,\n",
        "                COUNT(id) cnt_vacancies\n",
        "            FROM public.vacancies\n",
        "            GROUP BY schedule, employment\n",
        "            ORDER BY count(id) desc\n",
        "            '''\n",
        "            \n",
        "# выводим результат запроса\n",
        "schedule = pd.read_sql_query(query_4_4, connection)\n",
        "schedule"
      ]
